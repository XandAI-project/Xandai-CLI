      "content": "<actions>\nmkdir twitter-clone-fullstack\ncd twitter-clone-fullstack\n</actions>\n\n<code filename=\"package.json\">\n{\n  \"name\": \"twitter-clone-backend\",\n  \"version\": \"1.0.0\",\n  \"description\": \"Full-stack Twitter clone with Express.js backend and HTML/CSS/JS frontend\",\n  \"main\": \"server.js\",\n  \"scripts\": {\n    \"start\": \"node server.js\",\n    \"dev\": \"nodemon server.js\"\n  },\n  \"dependencies\": {\n    \"express\": \"^4.18.2\",\n    \"cors\": \"^2.8.5\",\n    \"mongoose\": \"^7.5.0\",\n    \"bcryptjs\": \"^2.4.3\",\n    \"jsonwebtoken\": \"^9.0.2\",\n    \"dotenv\": \"^16.3.1\"\n  },\n  \"devDependencies\": {\n    \"nodemon\": \"^3.0.1\"\n  }\n}\n</code>\n\n<code filename=\"server.js\">\nconst express = require('express');\nconst cors = require('cors');\nconst mongoose = require('mongoose');\nrequire('dotenv').config();\n\nconst app = express();\nconst PORT = process.env.PORT || 5000;\n\n// Middleware\napp.use(cors());\napp.use(express.json());\n\n// Connect to MongoDB\nmongoose.connect(process.env.MONGODB_URI || 'mongodb://localhost:27017/twitter-clone', {\n  useNewUrlParser: true,\n  useUnifiedTopology: true,\n})\n.then(() => console.log('Connected to MongoDB'))\n.catch((err) => console.error('MongoDB connection error:', err));\n\n// Routes\napp.get('/', (req, res) => {\n  res.json({ message: 'Twitter Clone API is running' });\n});\n\n// Start server\napp.listen(PORT, () => {\n  console.log(`Server is running on port ${PORT}`);\n});\n</code>\n\n<actions>\nmkdir public\ncd public\n</actions>\n\n<code filename=\"index.html\">\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\"/>\n  <title>Twitter Clone</title>\n  <link rel=\"stylesheet\" href=\"styles.css\">\n</head>\n<body>\n  <header class=\"header\">\n    <h1>Twitter Clone</h1>\n  </header>\n\n  <main class=\"container\">\n    <section class=\"tweet-compose\">\n    