          <TextField\n            margin=\"normal\"\n            required\n            fullWidth\n            id=\"email\"\n            label=\"Email Address\"\n            name=\"email\"\n            autoComplete=\"email\"\n            autoFocus\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n          />\n          <TextField\n            margin=\"normal\"\n            required\n            fullWidth\n            name=\"password\"\n            label=\"Password\"\n            type=\"password\"\n            id=\"password\"\n            autoComplete=\"current-password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n          <Button\n            type=\"submit\"\n            fullWidth\n            variant=\"contained\"\n            sx={{ mt: 3, mb: 2 }}\n          >\n            Sign In\n          </Button>\n        </Box>\n      </StyledPaper>\n    </Container>\n  );\n};\n\nexport default LoginScreen;\n</code>\n\n<code filename=\"src/components/RideSelector.tsx\">\nimport React, { useState } from 'react';\nimport {\n  Box,\n  Button,\n  Card,\n  CardContent,\n  Typography,\n  styled\n} from '@mui/material';\n\nconst StyledCard = styled(Card)(({ theme }) => ({\n  margin: theme.spacing(2),\n  cursor: 'pointer',\n  transition: '0.3s',\n  '&:hover': {\n    transform: 'scale(1.02)',\n    boxShadow: theme.shadows[5]\n  }\n}));\n\nconst RideSelector: React.FC = () => {\n  const [selectedRide, setSelectedRide] = useState<string | null>(null);\n\n  // Mock ride data\n  const rides = [\n    {\n      id: 'standard',\n      name: 'UberX',\n      description: 'Economy rides for everyday trips',\n      price: '$12.50',\n      time: '5 min',\n      icon: 'üöó'\n    },\n    {\n      id: 'premium',\n      name: 'UberXL',\n      description: 'Spacious rides for groups',\n      price: '$24.75',\n      time: '8 min',\n      icon: 'üöô'\n    },\n    {\n      id: 'comfort',\n      name: 'Comfort',\n      description: 'Climate-controlled rides',\n      price: '$18.25',\n      time: '6 min',\n      icon: 'üöï'\n    }\n  ];\n\n  const handleSelectRide = (id: string) => {\n    setSelectedRide(id);\n    console.log('Selected ride:', id);\n  };\n\n  return (\n    <Box sx={{ p: 2 }}>\n      <Typography variant=\"h5\" component=\"h2\" sx={{ mb: 3, textAlign: 'center' }}>\n        Choose a Ride\n      </Typography>\n      {rides.map((ride) => (\n        <StyledCard \n          key={ride.id} \n          onClick={() => handleSelectRide(ride.id)}\n          raised={selectedRide === ride.id}\n        >\n          <CardContent>\n            <Box display=\"flex\" alignItems=\"center\">\n              <Typography variant=\"h4\" sx={{ mr: 2 }}>\n                {ride.icon}\n              </Typography>\n              <Box>\n                <Typography variant=\"h6\">{ride.name}</Typography>\n                <Typography variant=\"body2\" color=\"text.secondary\">\n                  {ride.description}\n                </Typography>\n              </Box>\n            </Box>\n            <Box display=\"flex\" justifyContent=\"space-between\" sx={{ mt: 2 }}>\n              <Typography variant=\"h6\">{ride.price}</Typography>\n              <Typography variant=\"body2\" color=\"text.secondary\">\n                ETA: {ride.time}\n              </Typography>\n            </Box>\n          </CardContent>\n        </StyledCard>\n      ))}\n      \n      <Button \n        variant=\"contained\" \n        fullWidth \n        size=\"large\"\n        sx={{ mt: 4, py: 2 }}\n        disabled={!selectedRide}\n      >\n        Confirm Ride\n      </Button>\n    </Box>\n  );\n};\n\nexport default RideSelector;\n</code>\n\n<code filename=\"src/components/MiniMap.tsx\">\nimport React from 'react';\nimport {\n  Box,\n  Typography,\n  styled\n} from '@mui/material';\n\nconst StyledMap = styled(Box)(({ theme }) => ({\n  width: '100%',\n  height: 300,\n  backgroundColor: '#e0e0e0',\n  borderRadius: theme.shape.borderRadius,\n  position: 'relative',\n  overflow: 'hidden',\n  border: `1px solid ${theme.palette.grey[300]}`\n}));\n\nconst DriverMarker = styled(Box)(({ theme }) => ({\n  position: 'absolute',\n  width: 24,\n  height: 24,\n  backgroundColor: theme.palette.primary.main,\n  borderRadius: '50%',\n  top: '50%',\n  left: '50%',\n  transform: 'translate(-50%, -50%)',\n  display: 'flex',\n  alignItems: 'center',\n  justifyContent: 'center',\n  color: 'white',\n  fontWeight: 'bold',\n  boxShadow: theme.shadows[2]\n}));\n\nconst DestinationMarker = styled(Box)(({ theme }) => ({\n  position: 'absolute',\n  width: 30,\n  height: 30,\n  backgroundColor: '#ff4d4d',\n  borderRadius: '50%',\n  top: '70%',\n  left: '80%',\n  transform: 'translate(-50%, -50%)',\n  display: 'flex',\n  alignItems: 'center',\n  justifyContent: 'center',\n  color: 'white',\n  fontWeight: 'bold',\n  boxShadow: theme.shadows[2]\n}));\n\nconst MiniMap: React.FC = () => {\n  return (\n    <Box sx={{ p: 2 }}>\n      <Typography variant=\"h6\" component=\"h3\" sx={{ mb: 2 }}>\n        Your Ride\n      </Typography>\n      <StyledMap>\n        {/* Simulated road */}\n        <Box \n          sx={{ \n            position: 'absolute', \n            top: '50%', \n            left: 0, \n            right: 0, \n            height: 4,\n            backgroundColor: '#999' \n          }} \n        />\n        \n        {/* Driver marker (current location) */}\n        <DriverMarker>üöó</DriverMarker>\n        \n        {/* Destination marker */}\n        <DestinationMarker>üìç</DestinationMarker>\n      </StyledMap>\n      \n      <Box sx={{ mt: 2, textAlign: 'center' }}>\n        <Typography variant=\"body1\" color=\"text.secondary\">\n          Driver is 2.5 km away\n        </Typography>\n        <Typography variant=\"h6\" fontWeight=\"bold\">\n          Estimated arrival: 8 min\n        </Typography>\n      </Box>\n    </Box>\n  );\n};\n\nexport default MiniMap;\n</code>\n\n<code filename=\"src/App.tsx\">\nimport React from 'react';\nimport { BrowserRouter as Router, Routes, Route } from 'react-router-dom';\nimport LoginScreen from './components/LoginScreen';\nimport RideSelector from './components/RideSelector';\nimport MiniMap from './components/MiniMap';\nimport { ThemeProvider, createTheme } from '@mui/material/styles';\nimport CssBaseline from '@mui/material/CssBaseline';\n\n// Create a theme\nconst theme = createTheme({\n  palette: {\n    primary: {\n      main: '#000000', // Uber black\n    },\n    secondary: {\n      main: '#FFD700', // Uber yellow\n    },\n  },\n  typography: {\n    fontFamily: '\"Roboto\", \"Helvetica\", \"Arial\", sans-serif',\n    h1: {\n      fontWeight: 700,\n    },\n    h2: {\n      fontWeight: 600,\n    },\n  },\n});\n\nfunction App() {\n  return (\n    <ThemeProvider theme={theme}>\n      <CssBaseline />\n      <Router>\n        <div className=\"App\">\n          <Routes>\n            <Route path=\"/\" element={<LoginScreen />} />\n            <Route path=\"/rides\" element={<RideSelector />} />\n            <Route path=\"/map\" element={<MiniMap />} />\n          </Routes>\n        </div>\n      </Router>\n    </ThemeProvider>\n  );\n}\n\nexport default App;\n</code>\n\n<code filename=\"src/App.test.tsx\">\nimport React from 'react';\nimport { render, screen } from '@testing-library/react';\nimport App from './App';\n\ntest('renders learn react link', () => {\n  render(<App />);\n  const linkElement = screen.getByText(/learn react/i);\n  expect(linkElement).toBeInTheDocument();\n});\n</code>\n\n<code filename=\"src/index.tsx\">\nimport React from 'react';\nimport ReactDOM from 'react-dom/client';\nimport './index.css';\nimport App from './App';\nimport reportWebVitals from './reportWebVitals';\n\nconst root = ReactDOM.createRoot(\n  document.getElementById('root') as HTMLElement\n);\nroot.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>\n);\n\n// If you want to start measuring performance in your app, pass a function\n// to log results (for example: reportWebVitals(console.log))\n// or send to an analytics endpoint. Learn more: https://bit.ly/CRA-vitals\nreportWebVitals();\n</code>\n\n<code filename=\"src/reportWebVitals.ts\">\nimport { ReportHandler } from 'web-vitals';\n\nconst reportWebVitals = (onPerfEntry?: ReportHandler) => {\n  if (onPerfEntry && onPerfEntry instanceof Function) {\n    import('web-vitals').then(({ getCLS, getFID, getFCP, getLCP, getTTFB }) => {\n      getCLS(onPerfEntry);\n      getFID(onPerfEntry);\n      getFCP(onPerfEntry);\n      getLCP(onPerfEntry);\n      getTTFB(onPerfEntry);\n    });\n  }\n};\n\nexport default reportWebVitals;\n</code>\n\n<code filename=\"src/setupTests.ts\">\n// jest-dom adds custom jest matchers for asserting on DOM nodes.\n// allows you to do things like:\n// expect(element).toHaveTextContent(/react/i)\n// learn more: https://github.com/testing-library/jest-dom\nimport '@testing-library/jest-dom';\n</code>\n\nNow let me update the main index.css file to include some basic styling:\n\n<code filename=\"src/index.css\">\nbody {\n  margin: 0;\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen',\n    'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans', 'Helvetica Neue',\n    sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  background-color: #f5f5f5;\n}\n\ncode {\n  font-family: source-code-pro, Menlo, Monaco, Consolas, 'Courier New',\n    monospace;\n}\n</code>\n\nLet me also create a simple navigation component to make the app more functional:\n\n<code filename=\"src/components/Navigation.tsx\">\nimport React from 'react';\nimport { AppBar, Toolbar, Typography, Button } from '@mui/material';\nimport { useNavigate } from 'react-router-dom';\n\nconst Navigation: React.FC = () => {\n  const navigate = useNavigate();\n\n  return (\n    <AppBar position=\"static\" sx={{ backgroundColor: '#000000' }}>\n      <Toolbar>\n        <Typography variant=\"h6\" component=\"div\" sx={{ flexGrow: 1 }}>\n          Uber UI Clone\n        </Typography>\n        <Button \n          color=\"inherit\" \n          onClick={() => navigate('/')}\n        >\n          Home\n        </Button>\n        <Button \n          color=\"inherit\" \n          onClick={() => navigate('/rides')}\n        >\n          Rides\n        </Button>\n        <Button \n          color=\"inherit\" \n          onClick={() => navigate('/map')}\n        >\n          Map\n        </Button>\n      </Toolbar>\n    </AppBar>\n  );\n};\n\nexport default Navigation;\n</code`\n\nNow I'll update the App.tsx to include navigation:\n\n<code filename=\"src/App.tsx\">\nimport React from 'react';\nimport { BrowserRouter as Router, Routes, Route, Navigate } from 'react-router-dom';\nimport LoginScreen from './components/LoginScreen';\nimport RideSelector from './components/RideSelector';\nimport MiniMap from './components/MiniMap';\nimport Navigation from './components/Navigation';\nimport { ThemeProvider, createTheme } from '@mui/material/styles';\nimport CssBaseline from '@mui/material/CssBaseline';\n\n// Create a theme\nconst theme = createTheme({\n  palette: {\n    primary: {\n      main: '#000000', // Uber black\n    },\n    secondary: {\n      main: '#FFD700', // Uber yellow\n    },\n  },\n  typography: {\n    fontFamily: '\"Roboto\", \"Helvetica\", \"Arial\", sans-serif',\n    h1: {\n      fontWeight: 700,\n    },\n    h2: {\n      fontWeight: 600,\n    },\n  },\n});\n\nfunction App() {\n  return (\n    <ThemeProvider theme={theme}>\n      <CssBaseline />\n      <Router>\n        <div className=\"App\">\n          <Navigation />\n          <Routes>\n            <Route path=\"/\" element={<LoginScreen />} />\n            <Route path=\"/rides\" element={<RideSelector />} />\n            <Route path=\"/map\" element={<MiniMap />} />\n            <Route path=\"*\" element={<Navigate to=\"/\" replace />} />\n          </Routes>\n        </div>\n      </Router>\n    </ThemeProvider>\n  );\n}\n\nexport default App;\n</code>\n\nLet's run the application to make sure everything works:\n\n<actions>\nnpm start\n</actions>",
