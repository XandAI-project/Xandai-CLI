);\n});\n\n// API Routes\n\n// Get all users\napp.get('/api/users', (req, res) => {\n  const sql = 'SELECT id, username, displayName FROM users';\n  db.all(sql, [], (err, rows) => {\n    if (err) {\n      res.status(500).json({ error: err.message });\n      return;\n    }\n    res.json(rows);\n  });\n});\n\n// Get all tweets\napp.get('/api/tweets', (req, res) => {\n  const sql = `\n    SELECT t.id, u.username, u.displayName, t.content, t.timestamp\n    FROM tweets t\n    JOIN users u ON t.userId = u.id\n    ORDER BY t.timestamp DESC\n  `;\n  db.all(sql, [], (err, rows) => {\n    if (err) {\n      res.status(500).json({ error: err.message });\n      return;\n    }\n    res.json(rows);\n  });\n});\n\n// Create a new tweet\napp.post('/api/tweets', (req, res) => {\n  const { userId, content } = req.body;\n\n  if (!userId || !content) {\n    return res.status(400).json({ error: 'User ID and content are required' });\n  }\n\n  // Insert tweet\n  const sql = 'INSERT INTO tweets (userId, content) VALUES (?, ?)';\n  db.run(sql, [userId, content], function(err) {\n    if (err) {\n      return res.status(500).json({ error: err.message });\n    }\n\n    const tweetId = this.lastID;\n\n    // Extract hashtags from content\n    const hashtags = content.match(/#[a-zA-Z0-9_]+/g);\n    if (hashtags) {\n      hashtags.forEach(tag => {\n        const cleanTag = tag.substring(1); // Remove # prefix\n\n        // Insert hashtag if not exists\n        db.run('INSERT OR IGNORE INTO hashtags (tag) VALUES (?)', [cleanTag]);\n\n        // Get hashtag ID and link to tweet\n        db.get('SELECT id FROM hashtags WHERE tag = ?', [cleanTag], (err, row) => {\n          if (!err && row) {\n            db.run('INSERT OR IGNORE INTO tweet_hashtags (tweetId, hashtagId) VALUES (?, ?)', \n              [tweetId, row.id]);\n          }\n        });\n      });\n    }\n\n    res.status(201).json({ id: tweetId, content, userId });\n  });\n});\n\n// Get tweets by hashtag\napp.get('/api/tweets/hashtag/:tag'